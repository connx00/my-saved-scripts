local function vu13(p5, p6)
    local v7 = game.Players.LocalPlayer
    local v8 = v7.Backpack:FindFirstChild(p5) or v7.Character:FindFirstChild(p5)
    if v8 then
        local v9, v10, v11 = pairs(p6)
        while true do
            local v12
            v11, v12 = v9(v10, v11)
            if v11 == nil then
                break
            end
            v8:SetAttribute(v11, v12)
        end
        print(p5 .. " attributes have been set.")
    else
        print(p5 .. " not found in the Backpack or equipped.")
    end
end
